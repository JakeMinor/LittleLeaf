{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build --base=/LittleLeaf/",
    "test": "cypress run --browser chrome && vitest run --coverage",
    "test:e2e": "cypress open --browser chrome",
    "test:unit": "vitest --ui --coverage",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.4.19",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitest/coverage-v8": "^1.3.1",
    "@vitest/ui": "^1.3.1",
    "autoprefixer": "^10.4.18",
    "cypress": "^13.6.6",
    "daisyui": "^4.7.2",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "unplugin-vue-router": "^0.8.4",
    "vite": "^5.1.4",
    "vite-plugin-istanbul": "^5.0.0",
    "vitest": "^1.3.1",
    "vue-tsc": "^1.8.27"
  },
  "nyc": {
    "all": true,
    "report-dir": "coverage/cypress",
    "include": "*",
    "exclude": "**/*.cy.ts",
    "extension": [
      ".js",
      ".cjs",
      ".mjs",
      ".ts",
      ".tsx",
      ".jsx",
      ".vue"
    ],
    "reporter": [
      "json",
      "cobertura"
    ]
  }
}
